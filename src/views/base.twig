{% if pagetype == 'contentnosubnav' %}{%
set widecontent = true %}{%
else %}{%
set widecontent = false %}{%
endif %}{%
import "macros.twig" as macro %}<!DOCTYPE html>
<html lang="{{language}}">
<head>
    <meta charset="utf-8">
    <meta name="author" content="Marcus Haase">
    {% if item.data.itm_data.pagetitle is defined and item.data.itm_data.pagetitle != '' %}
        <title>{{item.data.itm_data.pagetitle}}</title>
    {% else %}
        <title>{{pagetitle}}</title>
    {% endif %}
    <meta name="revisit-after" content="5 days">
    <meta name="Robots" content="INDEX,FOLLOW">
    {% if keywords is defined %}
        <meta name="Keywords" content="{{keywords}}">
    {% elseif item.data.itm_data.pagekeywords is defined and item.data.itm_data.pagekeywords != '' %}
        <meta name="Keywords" content="{{item.data.itm_data.pagekeywords}}">
    {% endif %}
    {% if description is defined and description != '' %}
        <meta name="Description" content="{{description}}">
    {% elseif item.data.itm_data.pagedescription is defined and item.data.itm_data.pagedescription != '' %}
        <meta name="Description" content="{{item.data.itm_data.pagedescription}}">
    {% endif %}
    <link rel="icon" href="/favicon.ico" type="image/vnd.microsoft.icon">
    <link rel="stylesheet" href="//cdn.haase-it.com/purecss/0.6.0/pure-min.css">
    <link rel="stylesheet" href="//cdn.haase-it.com/purecss/0.6.0/grids-responsive-min.css">
    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="/global.css">
    <link rel="stylesheet" href="/_libs/modal.css">
    <link type="text/css" rel="stylesheet" href="/_libs/jquery.dropdown.css">
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="//oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="//oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    {% if head_scripts is defined %}{{head_scripts}}{% endif %}
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <script src="//code.jquery.com/jquery-1.11.2.min.js"></script>
</head>
<body>
<div class="pure-g">
    <div id="cookienoticecont" style="display: none;" class="pure-u-1">
        <div id="cookienotice" class="fs">{{ T("cookienotice_info_text") }}</div>
    </div>
</div>
<div class="pure-g" id="row-topbar">
    <div class="container">
        <div class="pure-u-1 pure-u-md-3-4" id="logo-head">
            <a href="/"><img src="/_img/haase-it-logo.gif"></a>
        </div>
        <div class="pure-u-1 pure-u-md-1-4">
            {% if langselector is defined %}<div id="headlangselector">{{langselector}}</div>{% endif %}
            {% if enable_module_shop %}<div class="pure-u-1-3 pure-u-md-1 button-head-container">
                <button class="pure-button pure-u-23-24 pure-u-md-1 button-orange button-head" data-dropdown="#dropdown-1">
                    <i class="fa fa-shopping-cart"></i>
                    <span id="shoppingcartamount" class="hidden-sm">{{cartinfo.numberofitems}} {{ T("utilities_shoppingcart_numberofitems") }}, {{ cartinfo.cartsumbrutto|number_format(2, ',', '.') }} {{ currency }}</span></button>
                <div id="dropdown-1" class="dropdown dropdown-tip">
                    <div class="dropdown-panel fs">
                        <ul id="shoppingcartinfo">
                            {% if cartinfo.cartitems is defined and cartinfo.cartitems|length > 0 %}
                                {% for item in cartinfo.cartitems %}
                                    <li>
                                        <a href="/_misc/shoppingcart.html">
                                            <b>{{ item.amount }}x {{ item.name }}</b>
                                            <span id="shoppingcartsum">{{ item.price.brutto_use|number_format(2, ',', '.') }}&nbsp;{{ currency }}</span>
                                        </a>
                                    </li>
                                {% endfor %}
                                <li class="divider"></li>
                                <li><a href="/_misc/shoppingcart.html">{{ T("utilities_shoppingcart_total") }} <span id="shoppingcartsum">{{ cartinfo.cartsumbrutto|number_format(2, ',', '.') }}</span> {{ currency }}</a></li>
                                <li class="divider"></li>
                                <li><a href="/_misc/shoppingcart.html" class="pure-button button-green">{{ T("utilities_gotoshoppingcart") }}</a></li>
                            {% else %}
                                <li>
                                    <a href="#">{{ T("utilities_shoppingcart_empty") }}</a>
                                </li>
                            {% endif %}
                        </ul>
                    </div></div></div>{%
            endif %}<div class="pure-u-1-3 pure-u-md-1 button-head-container" id="button-head-topnav">
                <button class="pure-button pure-u-23-24 pure-u-md-1 button-orange button-head">
                    <i class="fa fa-list"></i>
                    <span class="hidden-sm">Navigation</span></button>
            </div>
            {% if enable_module_shop %}
                <div id="searchbox" class="pure-u-1">
                    <form action="/_misc/suche.html" method="GET" name="search">
                        <input type="text" id="search" name="searchtext" value="{{gFF('searchtext', '')}}" class="formtextsearch" style="width:151px;" placeholder="{{T("misc_suche")}}">
                        <input type="image" name="searchsubmit" src="data:image/gif;base64," alt="Submit" style="vertical-align: -5px;padding-left:5px;" class="formsearchsubmit">
                    </form>
                </div>
            {% endif %}
        </div>
    </div>
</div>
<div class="pure-g">
    <div id="row-topnav" class="pure-u-1">
        <div class="container">
            <div class="fs pure-menu-horizontal">
                <ul id="topnav">
                    <li class="pure-u-1 pure-u-md-4-24"><a href="/kategorie1/" id="topnav-1"{{ subnavkey == 'category1' ? ' class="active"' : '' }}>{{ T("topnav_category1") }}</a>
                        {% if subnavkey == 'category1' %}
                            <ul id="topnav-sub">
                            {% if subnav is defined %}
                                {% for title, link in subnav %}
                                    <li class="pure-u-1"><a href="{{link}}"{% if requesturi matches '~^'~link~'item/([0-9a-zA-Z]+)\\.html(\\?.+)?~' or requesturi matches '~^'~link~'(index.html)?(\\?.+)?~' %} class="active"{% endif %}>{{title}}</a></li>
                                {% endfor %}
                            {% endif %}
                            </ul>{% endif %}</li>
                    <li class="pure-u-1 pure-u-md-4-24"><a href="/kategorie2/" id="topnav-2"{{ subnavkey == 'category2' ? ' class="active"' : '' }}>{{ T("topnav_category2") }}</a>
                        {% if subnavkey == 'category2' %}
                            <ul id="topnav-sub">
                            {% if subnav is defined %}
                                {% for title, link in subnav %}
                                    <li class="pure-u-1"><a href="{{link}}"{% if requesturi matches '~^'~link~'item/([0-9a-zA-Z]+)\\.html(\\?.+)?~' or requesturi matches '~^'~link~'(index.html)?(\\?.+)?~' %} class="active"{% endif %}>{{title}}</a></li>
                                {% endfor %}
                            {% endif %}
                            </ul>{% endif %}</li>
                    <li class="pure-u-1 pure-u-md-4-24"><a href="/kategorie3/" id="topnav-3"{{ subnavkey == 'category3' ? ' class="active"' : '' }}>{{ T("topnav_category3") }}</a>
                        {% if subnavkey == 'category3' %}
                            <ul id="topnav-sub">
                            {% if subnav is defined %}
                                {% for title, link in subnav %}
                                    <li class="pure-u-1"><a href="{{link}}"{% if requesturi matches '~^'~link~'item/([0-9a-zA-Z]+)\\.html(\\?.+)?~' or requesturi matches '~^'~link~'(index.html)?(\\?.+)?~' %} class="active"{% endif %}>{{title}}</a></li>
                                {% endfor %}
                            {% endif %}
                            </ul>{% endif %}</li>
                    <li class="pure-u-1 pure-u-md-4-24"><a href="/kategorie4/" id="topnav-4"{{ subnavkey == 'category4' ? ' class="active"' : '' }}>{{ T("topnav_category4") }}</a>
                        {% if subnavkey == 'category4' %}
                            <ul id="topnav-sub">
                            {% if subnav is defined %}
                                {% for title, link in subnav %}
                                    <li class="pure-u-1"><a href="{{link}}"{% if requesturi matches '~^'~link~'item/([0-9a-zA-Z]+)\\.html(\\?.+)?~' or requesturi matches '~^'~link~'(index.html)?(\\?.+)?~' %} class="active"{% endif %}>{{title}}</a></li>
                                {% endfor %}
                            {% endif %}
                            </ul>{% endif %}</li>
                    <li class="pure-u-1 pure-u-md-4-24"><a href="/kategorie5/" id="topnav-5"{{ subnavkey == 'category5' ? ' class="active"' : '' }}>{{ T("topnav_category5") }}</a>
                        {% if subnavkey == 'category5' %}
                            <ul id="topnav-sub">
                            {% if subnav is defined %}
                                {% for title, link in subnav %}
                                    <li class="pure-u-1"><a href="{{link}}"{% if requesturi matches '~^'~link~'item/([0-9a-zA-Z]+)\\.html(\\?.+)?~' or requesturi matches '~^'~link~'(index.html)?(\\?.+)?~' %} class="active"{% endif %}>{{title}}</a></li>
                                {% endfor %}
                            {% endif %}
                            </ul>{% endif %}</li>
                    <li class="pure-u-1 pure-u-md-3-24"><a href="/faq.html" id="topnav-6"{{ requesturi == '/faq.html' ? ' class="active"' : '' }}>{{ T("topnav_faq") }}</a></li>
                    <li class="pure-u-1 pure-u-md-1-24"><a href="/_misc/login.html" id="topnav-7"><i class="fa fa-user"></i> <span class="only-sm">Login / Register</span></a></li>
                </ul>
            </div>
        </div>
    </div>
</div>
<div class="pure-g" id="row-main">
    <div class="container">
        {% if widecontent == false %}
        <div class="pure-u-1 pure-u-md-1-4" id="col-leftnav">
            <div id="col-leftnav-content">
                <div class="pure-menu fs" id="leftnav">
                    {% if subnav is defined %}
                        <ul class="pure-menu-list">
                            {% for title, link in subnav %}
                                <li class="pure-menu-item{% if requesturi matches '~^'~link~'item/([0-9a-zA-Z]+)\\.html(\\?.+)?~' or requesturi matches '~^'~link~'(index.html)?(\\?.+)?~' %} pure-menu-selected{% endif %}"><a href="{{link}}" class="pure-menu-link">{{title}}</a></li>
                            {% endfor %}
                        </ul>
                    {% endif %}
                </div>
            </div>
        </div>
        <div class="pure-u-1 pure-u-md-3-4">
            <div id="col-main">
                {% else %}
                <div class="pure-u-1">
                    <div id="col-main">
                        {% endif %}

                        {% autoescape false %}
                        {% if (customcontenttemplate is defined and customcontenttemplate|trim != '') %}
                            <div class="fs">
                                {% include customcontenttemplate ~ '.twig' %}
                            </div>
                        {% else %}
                            {% if enable_module_shop %}
                                {% if pagetype == 'itemoverview' %}
                                    <div class="fs">
                                        {{content}}
                                        {% include 'shop/items-overview.twig' %}
                                    </div>
                                {% elseif pagetype == 'itemoverviewgrpd' %}
                                    <div class="fs">
                                        {{content}}
                                        {% include 'shop/items-overview-grouped.twig' %}
                                    </div>
                                {% elseif pagetype == 'itemdetail' %}
                                    <div class="fs">
                                        {% include 'shop/item-detail.twig' %}
                                    </div>
                                {% else %}
                                    <div class="fs">
                                        {{content}}
                                    </div>
                                {% endif %}
                            {% else %}
                                <div class="fs">
                                    {{content}}
                                </div>
                            {% endif %}
                        {% endif %}
                    </div>
                </div>
                {% endautoescape %}
                {% if enable_module_shop and pagetype == 'itemdetail' %}
                    {% include 'shop/item-detail-suggestions.twig' %}
                {% endif %}
            </div>
        </div>
        <div class="pure-g" id="row-footer"><div class="pure-u-1 fs" id="footer-text">{{ T("footer_copyright") }}</div></div>
        {{ debugdata }}
        <script type="text/javascript" src="/_libs/jquery.dropdown.js"></script>
        <script src="/_libs/bootstrap.min.js"></script>
        <script type="text/javascript">
            $(function() {

                $("#button-head-topnav").click(function(){
                    $("#row-topnav").toggle();
                });
                // Avoid `console` errors in browsers that lack a console.
                (function() {
                    var method;
                    var noop = function () {};
                    var methods = [
                        'assert', 'clear', 'count', 'debug', 'dir', 'dirxml', 'error',
                        'exception', 'group', 'groupCollapsed', 'groupEnd', 'info', 'log',
                        'markTimeline', 'profile', 'profileEnd', 'table', 'time', 'timeEnd',
                        'timeStamp', 'trace', 'warn'
                    ];
                    var length = methods.length;
                    var console = (window.console = window.console || {});

                    while (length--) {
                        method = methods[length];

                        // Only stub undefined methods.
                        if (!console[method]) {
                            console[method] = noop;
                        }
                    }
                }());
                function getCookie(cname) {
                    var name = cname + "=";
                    var ca = document.cookie.split(';');
                    for(var i=0; i<ca.length; i++) {
                        var c = ca[i];
                        while (c.charAt(0)==' ') c = c.substring(1);
                        if (c.indexOf(name) != -1) return c.substring(name.length,c.length);
                    }
                    return "";
                }
                var acceptscookies = getCookie('acceptscookies');
                if (acceptscookies == 'yes') {
                    console.log('You have accepted to use cookies on this site. To opt out again, please clear your cookies for this site.');
                } else {
                    function setCookie(cname, cvalue, exdays) {
                        var d = new Date();
                        d.setTime(d.getTime() + (exdays*24*60*60*1000));
                        var expires = "expires="+d.toUTCString();
                        document.cookie = cname + "=" + cvalue + "; path=/; " + expires;
                    }
                    $( "#cookienoticecont" ).slideDown( "slow", function() {
                        // Animation complete.
                    });
                    $("a").click(function(){
                        setCookie('acceptscookies', 'yes', 365);
                    });
                }
            });
        </script>
        <script src="/_libs/jquery.lazyload.min.js" type="text/javascript"></script>
        <script type="text/javascript" charset="utf-8">
            $(function () {
                $("img.lazy").lazyload({
                    effect: "fadeIn",
                    threshold: 200
                });
                $("[rel='tooltip']").tooltip();
            });
        </script>
</body>
</html>